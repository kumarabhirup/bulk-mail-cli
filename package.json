{
  "name": "bulk-mail-cli",
  "version": "0.5.0",
  "description": "Send automated, dynamic and beautifully designed HTML emails to your mailing list. Right from your terminal!",
  "main": "src/dist/src/bulkmailer.js",
  "author": "Kumar Abhirup",
  "scripts": {
    "start": "npm run build && node ./src/dist/src/bulkmailer.js",
    "build": "rimraf src/dist/ && babel ./ --out-dir src/dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log,./src/junkies,./LICENSE,./README.md,./package-lock.json --copy-files",
    "test": "echo No tests written.",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KumarAbhirup/bulk-mail-cli.git"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "array-includes": "^3.0.3",
    "csvtojson": "^2.0.8",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "nodemailer": "^4.7.0",
    "os-name": "^3.0.0",
    "simplesmtp": "^0.3.35",
    "terminal-kit": "^1.26.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.2"
  },
  "bundledDependencies": [
    "bulkmailcli_settings"
  ],
  "bin": {
    "bulkmail": "./src/dist/src/bin/global.js"
  },
  "preferGlobal": true,
  "keywords": [
    "mail",
    "bulk",
    "javascript",
    "nodemailer",
    "emailjs",
    "cli",
    "email",
    "node",
    "mailer"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/KumarAbhirup/bulk-mail-cli/issues"
  },
  "homepage": "https://github.com/KumarAbhirup/bulk-mail-cli#readme"
}
